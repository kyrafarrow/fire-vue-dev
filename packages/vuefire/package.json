{
  "name": "vuefire",
  "version": "2.0.0-alpha.11",
  "description": "Firestore bindings for Vue.js",
  "main": "dist/vuefire.common.js",
  "module": "dist/vuefire.esm.js",
  "unpkg": "dist/vuefire.js",
  "browser": "dist/vuefire.esm.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "types/*.d.ts",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint --color --ext=js,html src test examples",
    "build": "node --eval 'require(\"@posva/vuefire-bundler\")(\"Vuefire\")'",
    "pretest": "npm run lint",
    "test": "npm run unit",
    "types": "tsc -p ./types/test/tsconfig.json",
    "unit": "jest",
    "dev": "npm run unit -- --watchAll",
    "prepublishOnly": "npm run build",
    "release": "release-it pre --preReleaseId=alpha --npm.tag=next --github.preRelease"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vuefire.git"
  },
  "keywords": [
    "vue",
    "plugin",
    "mixin",
    "firebase",
    "cloud",
    "database",
    "google",
    "firestore",
    "realtime"
  ],
  "author": {
    "name": "Eduardo San Martin Morote"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/vuefire/issues"
  },
  "homepage": "https://github.com/vuejs/vuefire#readme",
  "peerDependencies": {
    "firebase": ">=4.0.0"
  },
  "devDependencies": {
    "@posva/vuefire-bundler": "^1.0.0",
    "@posva/vuefire-test-helpers": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-plugin-vue-libs": "^3.0.0",
    "jest": "^23.5.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/*.js"
    ]
  },
  "dependencies": {
    "@posva/vuefire-core": "^1.0.0"
  }
}
